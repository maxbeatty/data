{
  "name": "data",
  "version": "0.0.1",
  "description": "A simple helpful robot",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spbk/data.git"
  },
  "dependencies": {
    "botkit": "0.5.5",
    "joi": "^10.3.1",
    "winston": "2.3.1",
    "wreck": "^12.0.0"
  },
  "engines": {
    "node": "6.9.1",
    "npm": "3.10.8"
  },
  "scripts": {
    "lint": "eslint --ignore-path .gitignore .",
    "start": "node main.js",
    "pretest": "npm run lint",
    "test": "jest --coverage"
  },
  "bugs": {
    "url": "https://github.com/spbk/data/issues"
  },
  "homepage": "https://github.com/spbk/data#readme",
  "devDependencies": {
    "eslint": "^3.18.0",
    "eslint-config-airbnb-base": "^11.1.2",
    "eslint-plugin-import": "^2.2.0",
    "jest": "^20.0.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "collectCoverageFrom": [
      "lib/**/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  }
}
