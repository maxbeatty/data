{
  "name": "data",
  "version": "0.0.1",
  "description": "A simple helpful robot",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spbk/data.git"
  },
  "dependencies": {
    "botkit": "0.2.2"
  },
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=3.0.0"
  },
  "scripts": {
    "lint": "eslint --ignore-path .gitignore .",
    "start": "node main.js",
    "pretest": "npm run lint",
    "test": "jest --coverage"
  },
  "bugs": {
    "url": "https://github.com/spbk/data/issues"
  },
  "homepage": "https://github.com/spbk/data#readme",
  "devDependencies": {
    "eslint": "3.6.1",
    "eslint-config-airbnb-base": "8.0.0",
    "eslint-plugin-import": "1.16.0",
    "jest": "15.1.1",
    "sinon": "1.17.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "collectCoverageFrom": [
      "lib/**/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  }
}
